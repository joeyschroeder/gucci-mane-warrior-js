{
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": ["jest"],
  "rules": {
    "comma-dangle": 0,
    "import/prefer-default-export": [0],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
